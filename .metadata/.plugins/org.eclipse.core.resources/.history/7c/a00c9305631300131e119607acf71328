package charAt;

public class CharAtComponent {

	public static void main(String[] args) {
		String test1 = "Hello World!";
		String test2 = "";
		
		System.out.println(test1.length() - 1);
		System.out.println(test1.charAt(0));
		
		for (int k = 3; k == 0; k--){
			//test2 += currChar;
			System.out.println(test1.charAt(k));
		}

	}

}
